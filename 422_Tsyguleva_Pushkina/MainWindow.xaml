<Window x:Class="_422_Tsyguleva_Pushkina.MainWindow"
        Loaded="Window_Loaded"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_422_Tsyguleva_Pushkina"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="45"></RowDefinition>
            <RowDefinition ></RowDefinition>
            <RowDefinition Height="45"></RowDefinition>
        </Grid.RowDefinitions>
        <Frame x:Name="MainFrame" Grid.Row="1"   Grid.Column="0" Grid.ColumnSpan="6" NavigationUIVisibility="Hidden"></Frame>
        <TextBlock x:Name="DateTimeNow" Grid.Row="2" Grid.Column="4" Grid.ColumnSpan="2"/>
        <Button x:Name="nazad" Content="Назад" Grid.Column="0" Height="45"  Click="nazad_Click_1"/>
    </Grid>
</Window>
